module "dsci_benchmark_marketing_lambdas" {
  source                                       = "./lambdas"
  tags                                         = "${var.default_tags}"
  name_prefix                                  = "${local.name_prefix}"
  dsci_benchmark_bucket_name                   = "${local.dsci_benchmark_bucket_name}"
  dsci_benchmark_marketing_dynamodb_table_name = "${local.dsci_benchmark_marketing_dynamodb_table_name}"
  dsci_benchmark_bookmark_dynamodb_table_name  = "${local.dsci_benchmark_bookmark_dynamodb_table_name}"
  dsci_benchmark_eol_dynamodb_table_name       = "${local.dsci_benchmark_eol_dynamodb_table_name}"
  dsci_benchmark_eol_dynamodb_table_arn        = "${aws_dynamodb_table.dsci_benchmark_eol.arn}"
  dsci_benchmark_bookmark_dynamodb_table_arn   = "${aws_dynamodb_table.dynamodb_benchmark_bookmark_table.arn}"
  dsci_benchmark_marketing_dynamodb_table_arn  = "${aws_dynamodb_table.dsci_benchmark_marketing.arn}"
  region                                       = "${var.region}"
  dsci_benchmark_eol_api_id                    = "${aws_api_gateway_rest_api.dsci_benchmark_eol_api.id}"
  dsci_benchmark_eol_api_stage_name            = "${local.dsci_eol_benchmark_stage_name}"
  dsci_benchmark_marketing_api_id              = "${aws_api_gateway_rest_api.dsci_benchmark_marketing_api.id}"
  dsci_benchmark_marketing_api_stage_name      = "${local.dsci_marketing_benchmark_stage_name}"
  marketing_apigateway_stage_name              = "${local.dsci_marketing_benchmark_stage_name}"
  marketing_apigateway_api_key                 = "${aws_api_gateway_api_key.dsci_benchmark_marketing_api_gateway_key.value}"
  marketing_apigateway_resource_name           = "${var.dsci_benchmark_marketing_resource_name}"
  marketing_apigateway_rest_api_id             = "${aws_api_gateway_rest_api.dsci_benchmark_marketing_api.id}"
  eol_apigateway_rest_api_id                   = "${aws_api_gateway_rest_api.dsci_benchmark_eol_api.id}"
  eol_apigateway_api_key                       = "${aws_api_gateway_api_key.dsci_benchmark_eol_api_gateway_key.value}"
  eol_apigateway_resource_name                 = "${var.benchmark_eol_resource_name}"
  eol_apigateway_stage_name                    = "${local.dsci_eol_benchmark_stage_name}"
}
